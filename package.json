{
	"name": "ts-template",
	"private": true,
	"version": "0.0.0",
	"description": "A template repository for TypeScript projects",
	"author": "willclarktech <willclarktech@users.noreply.github.com>",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/willclarktech/ts-template.git"
	},
	"bugs": {
		"url": "https://github.com/willclarktech/ts-template/issues"
	},
	"homepage": "https://github.com/willclarktech/ts-template#readme",
	"keywords": [
		"template",
		"TypeScript"
	],
	"main": "dist/index.js",
	"types": "types/index.d.ts",
	"files": [
		"dist/",
		"types/",
		"*.md",
		"!*.spec.*"
	],
	"engines": {
		"node": ">=20",
		"npm": ">=10"
	},
	"scripts": {
		"format": "prettier --write src",
		"lint": "eslint \"**/*.{cjs,cts,js,mjs,mts,ts}\"",
		"test": "jest",
		"prebuild": "rm -rf dist/ types/",
		"build": "tsc"
	},
	"devDependencies": {
		"@types/jest": "^29.5.12",
		"@typescript-eslint/eslint-plugin": "^7.13.0",
		"@typescript-eslint/parser": "^7.13.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-config-willclarktech": "github:willclarktech/eslint-config-willclarktech.git#9470b92",
		"eslint-plugin-deprecation": "^3.0.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-functional": "^6.5.1",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-jest": "^28.6.0",
		"eslint-plugin-prettier": "^5.1.3",
		"jest": "^29.7.0",
		"prettier": "^3.3.2",
		"ts-jest": "^29.1.4",
		"tsconfig-willclarktech": "github:willclarktech/tsconfig-willclarktech.git#2ad3aba",
		"typescript": "~5.4.5"
	}
}
