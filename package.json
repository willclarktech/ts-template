{
	"name": "ts-template",
	"private": true,
	"version": "0.0.0",
	"description": "A template repository for TypeScript projects",
	"author": "willclarktech <willclarktech@users.noreply.github.com>",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/willclarktech/ts-template.git"
	},
	"bugs": {
		"url": "https://github.com/willclarktech/ts-template/issues"
	},
	"homepage": "https://github.com/willclarktech/ts-template#readme",
	"keywords": [
		"template",
		"TypeScript"
	],
	"main": "dist/index.js",
	"types": "types/index.d.ts",
	"files": [
		"dist/",
		"types/",
		"*.md",
		"!*.spec.*"
	],
	"scripts": {
		"format": "prettier --write src",
		"lint": "eslint \"**/*.{js,ts}\"",
		"test": "jest",
		"prebuild": "rm -rf dist/ types/",
		"build": "tsc"
	},
	"devDependencies": {
		"@types/jest": "^29.2.5",
		"@typescript-eslint/eslint-plugin": "^5.48.0",
		"@typescript-eslint/parser": "^5.48.0",
		"eslint": "^8.31.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-config-willclarktech": "github:willclarktech/eslint-config-willclarktech",
		"eslint-plugin-deprecation": "^1.3.3",
		"eslint-plugin-functional": "^4.4.1",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jest": "^27.2.1",
		"eslint-plugin-prettier": "^4.2.1",
		"jest": "^29.3.1",
		"prettier": "^2.8.1",
		"ts-jest": "^29.0.3",
		"tsconfig-willclarktech": "github:willclarktech/tsconfig-willclarktech",
		"typescript": "~4.9.4"
	}
}
